# Streamlit Cloud Secrets Template
# Copy this content to your Streamlit Cloud secrets settings
# (Settings > Secrets in your app dashboard)

# Choose your model provider: "groq" or "gemini"
MODEL_PROVIDER = "groq"

# Model name (depends on provider)
MODEL_NAME = "moonshotai/kimi-k2-instruct"

# API Keys - add the one matching your provider
GROQ_API_KEY = "your_groq_api_key_here"
GOOGLE_API_KEY = "your_google_api_key_here"
